#!/bin/bash

source creep/runes/runes.sh
source creep/creep.sh

log=runes.log

echo "hola"
runes.log "hola"
$log "hola"

# Reading the runes files
readarray RUNES < .creep/.runes

FILES=$(git.status)

for FILE in ${FILES[@]}; do
	FILE=${FILE#*  }
	if runes.isRune $FILE; then
		echo "Found a rune ${FILE}"
		#TODO: encrypt
	fi;
done
